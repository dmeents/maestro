{
  "name": "maestro",
  "version": "1.0.0",
  "description": "Repository for shared project configs like eslint, typescript, prettier, etc",
  "repository": "https://github.com/dmeents/maestro.git",
  "author": "David Meents <david@allido.io>",
  "license": "MIT",
  "files": [
    ".dist"
  ],
  "types": "./.dist/index.d.ts",
  "main": "./.dist/maestro.umd.js",
  "module": "./.dist/maestro.es.js",
  "exports": {
    ".": {
      "import": "./.dist/index.es.js",
      "require": "./.dist/index.cjs.js"
    }
  },
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "rimraf .dist && tsc && node ./esbuild.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1"
  },
  "dependenciesMeta": {
    "eslint": {
      "unplugged": true
    }
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "esbuild": "^0.14.48",
    "esbuild-node-externals": "^1.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
